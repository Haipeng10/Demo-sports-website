  <div class="container">
    <div class="data-form">
        <div class="container">
            <form action="/programs/create" method="post" class="fs-2">
                <h2>New Program:</h2>
                <div class="mb-3">
                    <label for="name" class="form-label">Name</label>
                    <input type="text" class="form-control fs-3" name="name" id="name" placeholder="Name" required />
                </div>
                <div class="mb-3">
                    <label for="description" class="form-label">Description</label>
                    <input type="text" class="form-control fs-3" name="description" id="description" placeholder="Description" required />
                </div>
                <div class="mb-3">
                    <label for="schedule" class="form-label">Schedule</label>
                    <input type="text" class="form-control fs-3" name="schedule" id="schedule" placeholder="Schedule" required />
                </div>
                <div class="mb-3">
                  <label for="instructor" class="form-label">Instructor</label>
                  <input type="text" class="form-control fs-3" name="instructor" id="instructor" placeholder="Instructor" required />
              </div>
                <button type="submit" class="btn btn-primary fs-3 mb-3">Submit</button>
            </form>
        </div>
    </div>
</div>
